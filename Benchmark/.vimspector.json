{                                                                                                      
  "configurations": {
    "cpp:launch": {
      "adapter": "vscode-cpptools",
      "configuration": {
	"name": "cpp:launch",
	// "stopAtEntry": true,
	"type": "cppdbg",
	"request": "launch",
	"program": "./Benchmark.out",
	"args": ["--configdir=../bin"], 
	"cmd": ".",
	"environment": [], 
	"externalConsole": true,
	"MIMode": "gdb",
	"breakpointers": {
	  "exception": {
	    "cpp_throw": "Y",
	    "cpp_catch": "N" 
	  }   
	}   
      }   
    }
  }   
}
